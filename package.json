{
  "author": "Glenn Jones",
  "name": "microformat-node",
  "description": "A microformat 2 parser for node.js",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "parser",
    "microfomats",
    "h-adr",
    "h-card",
    "h-entry",
    "h-event",
    "h-geo",
    "h-news",
    "h-org",
    "h-product",
    "h-recipe",
    "h-resume",
    "h-review-aggregate",
    "h-review"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/glennjones/microformat-node.git"
  },
  "engines": {
    "node": ">= 0.10.x"
  },
  "main": "index",
  "bin": {
    "microformat-node": "./bin/microformat-node"
  },
  "dependencies": {
    "cheerio": "0.19.x",
    "bluebird": "2.9.x",
    "microformat-shiv": "https://api.github.com/repos/glennjones/microformat-shiv/tarball/"
  },
  "devDependencies": {
    "hapi": "8.4.x",
    "blipp": "2.1.x",
    "joi": "6.6.x",
    "good": "6.1.x",
    "good-console": "5.0.x",
    "request": "2.55.x",
    "mocha": "2.2.x",
    "chai": "3.0.x",
    "download-github-repo": "0.1.x",
    "fs-extra": "0.19.x",
    "grunt": "0.4.x",
    "grunt-contrib-concat": "0.5.x",
    "grunt-contrib-copy": "0.8.x",
    "grunt-contrib-jshint": "0.11.x",
    "grunt-contrib-watch": "0.6.x",
    "jscoverage":"0.6.x",
    "coveralls": "2.11.x",
    "mocha-lcov-reporter": "0.0.x"
  },
  "scripts": {
    "start": "app",
    "test": "make test",
    "coveralls": "./node_modules/.bin/mocha test -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  }
}
